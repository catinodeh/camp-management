    <script src="@Url.Content("~/lib/datepicker/bootstrap-datepicker.js")"></script>
    <!-- select2 -->
    <script src="@Url.Content("~/lib/select2/select2.min.js")" cache="false"></script>
    <!-- maskedinput -->
